      "magento/module-catalog": "*",
      "magento/module-email": "*",
      "magento/module-ui": "*",
      "magento/module-variable": "*",
      "magento/module-media-storage": "*",
      "magento/framework": "*",
      "magento/magento-composer-installer": "*"
    },
    "type": "magento2-module",
